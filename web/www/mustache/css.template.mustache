[class*="icon-"] {
    display: inline-block;
    background-repeat: no-repeat;
    background-image: url('../img/min/sprite.png');
    background-color: transparent;
}

{{#items}}
.icon-{{name}} {
  background-position: {{px.offset_x}} {{px.offset_y}};
  width: {{px.width}};
  height: {{px.height}};
}
{{/items}}
